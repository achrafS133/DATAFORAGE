# DataForge Environment Configuration
# Rename this file to .env and adjust as needed

# Database Settings (PostgreSQL - optional, default is SQLite)
DB_TYPE=sqlite
DB_NAME=dataforge.db
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=postgres
# DB_PASS=password

# AI Configuration (Ollama)
OLLAMA_URL=http://localhost:11434/api/generate
OLLAMA_MODEL=qwen3:latest

# Generation Settings
BATCH_SIZE=10
WORKERS=2
USE_AI=true
